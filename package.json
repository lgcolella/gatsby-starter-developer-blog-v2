{
  "name": "gatsby-starter-developer-blog-v2",
  "description": "Blog starter made with Gatbsy.js and Typescript",
  "version": "1.0.0",
  "author": "lgcolella",
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "dev": "concurrently \"gatsby develop\" \"tsc --noEmit --watch\"",
    "start": "npm run dev",
    "serve": "gatsby serve",
    "check-type": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.[jt]s?(x)\"",
    "test": "npm run check-type && npm run test:jest",
    "test:jest": "jest",
    "coverage": "npm run test:jest -- --coverage"
  },
  "dependencies": {
    "gatsby": "^2.15.29",
    "gatsby-image": "^2.2.24",
    "gatsby-plugin-feed": "^2.3.15",
    "gatsby-plugin-i18n": "^1.0.1",
    "gatsby-plugin-less": "^3.0.9",
    "gatsby-plugin-manifest": "^2.2.20",
    "gatsby-plugin-offline": "^3.0.11",
    "gatsby-plugin-react-helmet": "^3.1.10",
    "gatsby-plugin-robots-txt": "^1.5.0",
    "gatsby-plugin-sharp": "^2.2.28",
    "gatsby-plugin-sitemap": "^2.2.16",
    "gatsby-plugin-typescript": "^2.1.11",
    "gatsby-remark-copy-linked-files": "^2.1.24",
    "gatsby-remark-images": "^3.1.25",
    "gatsby-remark-prismjs": "^3.3.17",
    "gatsby-source-filesystem": "^2.1.29",
    "gatsby-transformer-remark": "^2.6.27",
    "gatsby-transformer-sharp": "^2.2.20",
    "less": "^3.10.3",
    "prismjs": "^1.15.0",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-helmet": "^5.2.0",
    "react-icons": "^3.3.0",
    "react-share": "^3.0.1"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.6.0",
    "@types/chance": "^1.0.7",
    "@types/enzyme": "^3.10.3",
    "@types/jest": "^24.0.18",
    "@types/react": "^16.9.5",
    "@types/react-helmet": "^5.0.11",
    "@types/react-share": "^3.0.1",
    "babel-jest": "^24.9.0",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-preset-gatsby": "^0.2.17",
    "chance": "^1.1.0",
    "concurrently": "^5.0.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.13.2",
    "enzyme-to-json": "^3.4.2",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.9.0",
    "prettier": "^1.16.4",
    "prop-types": "^15.6.2",
    "typescript": "^3.6.3"
  },
  "peerDependencies": {
    "@babel/core": "^7.0.0",
    "webpack": "^4.0.0"
  }
}
